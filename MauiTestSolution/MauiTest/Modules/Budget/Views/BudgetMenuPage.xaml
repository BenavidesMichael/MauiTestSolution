<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       x:Class="MauiTest.Modules.Budget.Views.BudgetMenuPage"
       xmlns:Budget="clr-namespace:MauiTest.Modules.Budget.Views"
       NavigationPage.HasNavigationBar="False">

    <TabBar>
        <Tab Title="DashBoard">
            <ShellContent ContentTemplate="{DataTemplate Budget:BudgetPage}"/>
        </Tab>
        <Tab Title="Statistics">
            <ShellContent ContentTemplate="{DataTemplate Budget:StatisticsPage}" />
        </Tab>
    </TabBar>
    
</Shell>